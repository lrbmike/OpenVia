{
  "adapters": {
    "default": "telegram",
    "telegram": {
      "botToken": "your_bot_token_here",
      "allowedUserIds": [123456789, 987654321]
    },
    "feishu": {
      "appId": "your_app_id",
      "appSecret": "your_app_secret",
      "verificationToken": "optional_verification_token",
      "encryptKey": "optional_encrypt_key"
    }
  },
  "llm": {
    "format": "openai",
    "apiKey": "sk-xxx",
    "baseUrl": "https://dashscope.aliyuncs.com/compatible-mode/v1",
    "model": "qwen-max",
    "timeout": 120000,
    "maxTokens": 4096,
    "shellConfirmList": ["rm", "mv", "sudo", "su", "dd", "reboot", "shutdown"]
  },
  "logging": {
    "level": "info",
    "verbose": false
  }
}
